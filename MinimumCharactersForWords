using System;
using System.Collections.Generic;

public class Program {
   public string[] MinimumCharactersForWords(string[] words) {
       Dictionary<char, int> requiredChars= new Dictionary<char, int>();
     
          foreach(var word in words)
          {
              Dictionary<char, int> wordCount= new Dictionary<char, int>();
              foreach(var letter in word)
              {
                  
              
              if(!wordCount.ContainsKey(letter))
              {
                   wordCount[letter]= 1;
              }
                  else{
                      wordCount[letter]++;
                  }
             // wordCount[letter]++;
              
              }
          
          foreach(var ch in wordCount)
          {
              if(requiredChars.ContainsKey(ch.Key))
              {
                  requiredChars[ch.Key]= Math.Max(requiredChars[ch.Key], ch.Value);
                  
              }
              else{
                  requiredChars[ch.Key]= ch.Value;
              }

          }
          }
       
      List<string> res=new List<string>();
      foreach (var kvp in requiredChars) {
      for(int i=0;i<kvp.Value;i++)
      {
          res.Add(kvp.Key.ToString());
      }
      } 
         return res.ToArray();
  }
}
