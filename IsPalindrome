using System;
using System.Text;
public class Program {
	public static bool IsPalindrome(string str) {
        StringBuilder str2 = new StringBuilder();
		for(int i=str.Length-1;i>=0;i--)
        {
            str2.Append(str[i]);
        }
         //Console.WriteLine(str2.ToString());
       
        if(str2.ToString()==str)
        {
            return true;
        }
        return false;
		
	}
}
